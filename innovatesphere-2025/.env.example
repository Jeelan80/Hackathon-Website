# Environment Configuration Template
# Copy this file to .env.local and fill in your values

# Application Configuration
VITE_APP_NAME=InnovateSphere 2025
VITE_APP_VERSION=1.0.0
VITE_APP_DESCRIPTION=AI for Social Good Hackathon

# API Configuration
VITE_API_URL=https://api.innovatesphere.com

# Google Apps Script Configuration
# Replace these with your Google Apps Script Web App URL

# Your Google Apps Script Web App URL (get this after deploying your script)
VITE_GOOGLE_APPS_SCRIPT_URL=https://script.google.com/macros/s/your_script_id/exec

# Service Account Private Key (from Google Cloud Console)
# Make sure to replace \n with actual newlines
VITE_GOOGLE_PRIVATE_KEY="-----BEGIN PRIVATE KEY-----\nYOUR_PRIVATE_KEY_HERE\n-----END PRIVATE KEY-----\n"

# Optional: Enable Google Sheets integration
VITE_ENABLE_GOOGLE_SHEETS=true
VITE_API_VERSION=v1

# External Services

VITE_REGISTRATION_URL=https://forms.innovatesphere.com/register
VITE_CONTACT_EMAIL=director@btibangalore.com

# Social Media Links
VITE_TWITTER_URL=https://twitter.com/innovatesphere
VITE_LINKEDIN_URL=https://linkedin.com/company/innovatesphere
VITE_GITHUB_URL=https://github.com/innovatesphere

# Analytics and Monitoring
VITE_GOOGLE_ANALYTICS_ID=
VITE_SENTRY_DSN=
VITE_HOTJAR_ID=

# Feature Flags
VITE_ENABLE_ANALYTICS=true
VITE_ENABLE_ERROR_TRACKING=true
VITE_ENABLE_PERFORMANCE_MONITORING=true
VITE_ENABLE_ACCESSIBILITY_TESTING=false

# Development Settings
VITE_DEBUG_MODE=false
VITE_SHOW_PERFORMANCE_METRICS=false

# Deployment Settings
VITE_DEPLOYMENT_PLATFORM=netlify
VITE_BUILD_TIMESTAMP=